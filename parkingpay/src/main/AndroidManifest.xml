    <application>

        <uses-library
            android:name="org.apache.http.legacy"
            android:required="false" />

        <activity
            android:name=".ui.main.ParkingMainActivity"
            android:screenOrientation="portrait"
            android:theme="@style/ParkingDialogActivityTheme" />
        <activity
            android:name=".ui.parkings.ParkingParkingsActivity"
            android:screenOrientation="portrait"
            android:theme="@style/ParkingActivityTheme" />
        <activity
            android:name=".ui.payconfirm.ParkingPayConfirmActivity"
